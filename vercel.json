{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "bskyweb/build",
  "installCommand": "npm install",
  "framework": null,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "includeFiles": ["custom-files/**"],
        "distDir": "bskyweb/build"
      }
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}